<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>TPE1</servlet-name>
		<servlet-class>tpe.TPE1Servlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>TPE1</servlet-name>
		<url-pattern>/tpe1</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>Main.html</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>AlignmentServlet</servlet-name>
		<servlet-class>tpe.AlignmentServlet1</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AlignmentServlet</servlet-name>
		<url-pattern>/AlignmentServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>MultiAlignmentServlet</servlet-name>
		<servlet-class>tpe.MultiAlignmentServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>MultiAlignmentServlet</servlet-name>
		<url-pattern>/MultiAlignmentServlet</url-pattern>
	</servlet-mapping>
     
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
</web-app>
